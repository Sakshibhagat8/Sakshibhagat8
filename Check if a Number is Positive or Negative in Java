class Main{
    public static void main(String args[])
    {
        int num=5;
        if(num>0)
        {
            System.out.println("It is positive number");
        }
        else if(num<0)
        {
            System.out.println("It is negative number");
        }
        else
        {
            System.out.println("ZERO");
        }
    }
}
